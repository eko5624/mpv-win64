# See: man pkg-config
prefix=$pythonLocation
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Python
Description: Build a C extension for Python
Requires:
Version: 3.10
Libs.private: -ldl   -framework CoreFoundation
Libs: -L${libdir} -lpython3
Cflags: -I${includedir}
